<%- include("partials/header") -%>
<h1>Home</h1>
<p> <%= homeText %> </p>
<% newPosts.forEach(post => { %>
    <div>
       <h1><%= post.title %></h1>
        <p>
            <%= post.body.substring(0, 100) + " ..." %>
            <a href="/posts/<%=post.title.toLowerCase()%>">Read More</a>
        </p>
    </div>
        <% }) %>
<%- include("partials/footer") -%>